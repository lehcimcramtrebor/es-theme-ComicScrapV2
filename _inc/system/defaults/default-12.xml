<theme>
<formatVersion>7</formatVersion>

<view name="system">

	<image name="default.12" extra="true">
		<linearSmooth>true</linearSmooth>
		<origin>0.5 0.5</origin>
		<minSize>${s.12}</minSize>
		<pos>${p.12}</pos>
		<color>${tintTiles}</color>
		<zIndex>${di}</zIndex>
		<path>./../../../_comicScrapV2Custom/tiles/default/12.png</path>
		<path>./../../../_comicScrapV2Custom/tiles/default/12.jpg</path>
		<path>./${path.windows}/../../../../../_comicScrapV2Custom/tiles/default/12.png</path>
		<path>./${path.windows}/../../../../../_comicScrapV2Custom/tiles/default/12.jpg</path>

		<path ifSubset="forceCustoms:yes">./../../../_comicScrapV2Custom/tiles/${system.theme}/12.png</path>
		<path ifSubset="forceCustoms:yes">./../../../_comicScrapV2Custom/tiles/${system.theme}/12.jpg</path>
		<path ifSubset="forceCustoms:yes">./${path.windows}/../../../../../_comicScrapV2Custom/tiles/${system.theme}/12.png</path>
		<path ifSubset="forceCustoms:yes">./${path.windows}/../../../../../_comicScrapV2Custom/tiles/${system.theme}/12.jpg</path>
	</image>

</view>

</theme>
