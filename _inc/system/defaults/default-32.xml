<?xml version="1.0" encoding="UTF-8"?>
<!--
Theme: es-theme-ComicScrap for Retrobat - Batocera
version: 2.0
author: Lehcim Cram Trebor
inspired by ComicBook theme by TMNTturtleguy at https://github.com/TMNTturtleguy/es-theme-ComicBook
-->

<theme>
<formatVersion>7</formatVersion>

<view name="system">

	<image name="default.32" extra="true">
		<linearSmooth>true</linearSmooth>
		<origin>0.5 0.5</origin>
		<minSize>${s.32}</minSize>
		<pos>${p.32}</pos>
		<color>${tintTiles}</color>
		<zIndex>${di}</zIndex>
		<path>./../../../_comicScrapV2Custom/tiles/default/32.png</path>
		<path>./../../../_comicScrapV2Custom/tiles/default/32.jpg</path>
		<path>./${path.windows}/../../../../../_comicScrapV2Custom/tiles/default/32.png</path>
		<path>./${path.windows}/../../../../../_comicScrapV2Custom/tiles/default/32.jpg</path>

		<path ifSubset="forceCustoms:yes">./../../../_comicScrapV2Custom/tiles/${system.theme}/32.png</path>
		<path ifSubset="forceCustoms:yes">./../../../_comicScrapV2Custom/tiles/${system.theme}/32.jpg</path>
		<path ifSubset="forceCustoms:yes">./${path.windows}/../../../../../_comicScrapV2Custom/tiles/${system.theme}/32.png</path>
		<path ifSubset="forceCustoms:yes">./${path.windows}/../../../../../_comicScrapV2Custom/tiles/${system.theme}/32.jpg</path>
	</image>

</view>

</theme>
