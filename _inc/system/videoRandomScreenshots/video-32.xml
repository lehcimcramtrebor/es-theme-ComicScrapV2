<theme>
<formatVersion>7</formatVersion>

	<include>./../images/image-32.xml</include>

<view name="system">

	<video name="video.32" extra="true">
		<linearSmooth>true</linearSmooth>
		<origin>0.5 0.5</origin>
		<minSize>${s.32}</minSize>
		<pos>${p.32}</pos>
		<color>${tintTiles}</color>
		<zIndex>${vti}</zIndex>
		<delay>0.6</delay>
		<effect>none</effect>
		<audio>false</audio>

		<path>{random}</path>

	</video>

</view>

</theme>
